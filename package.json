{
  "name": "infoeste-scraping",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start": "npm run dev --workspace @infoeste/scraper",
    "dev": "npm run dev --workspace @infoeste/scraper",
    "build": "npm run build --workspace @infoeste/core && npm run build --workspace @infoeste/infrastructure && npm run build --workspace @infoeste/web && npm run build --workspace @infoeste/scraper",
    "lint": "biome check .",
    "fmt:fix": "npx @biomejs/biome format --write .",
    "clean": "node ./scripts/clean.mjs",
    "knip": "knip"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@types/node": "^24.8.0",
    "knip": "^5.65.0",
    "typescript": "^5.9.3"
  }
}
